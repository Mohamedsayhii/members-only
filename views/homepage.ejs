<link rel="stylesheet" href="style.css" />
<link rel="stylesheet" href="home-style.css" />
<title>Homepage</title>
<%- include('navbar') %>
<main>
	<ul>
		<% messages.forEach( (message) => { %>
		<li>
			<h3><%= message.username %></h3>
			<h3><%= message.title %></h3>
			<p><%= message.date %></p>
			<p><%= message.text %></p>
		</li>
		<% }); %>
	</ul>
</main>
<%- include('footer') %>
